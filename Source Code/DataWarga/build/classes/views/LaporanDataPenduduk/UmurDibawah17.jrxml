	<queryString language="SQL">
		<![CDATA[SELECT
     tpenduduk.`nik` AS NIK,
     tpenduduk.`namapen` AS Nama,
     tpenduduk.`jenis_kelamin` AS JenisKelamin,
     tpenduduk.`tempat_lahir` AS TempatLahir,
     tpenduduk.`tanggal_lahir` AS TanggalLahir,
     tpenduduk.`agama` AS Agama,
     tpenduduk.`pendidikan` AS Pendidikan,
     tpenduduk.`jenis_pekerjaan` AS JenisPekerjaan,
     tpenduduk.`alamat` AS Alamat,
     tpenduduk.`jenis_penduduk` AS JenisPenduduk,
     TIMESTAMPDIFF(YEAR,tpenduduk.`tanggal_lahir`,CURDATE()) AS Umur
FROM
     `tpenduduk` tpenduduk
WHERE
     TIMESTAMPDIFF(YEAR,tpenduduk.`tanggal_lahir`,CURDATE()) <= "16";]]>
	</queryString>
